#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	float a , b , temp;
	printf("Enter value of a: \n");
	scanf("%f",&a);
	printf("Enter value of b: \n");
	scanf("%f",&b);

	temp = a;
	a = b;
	b = temp;
	printf("Enter value of a: %f\n" , a);
	printf("Enter value of a: %f\n" , b);
	return 0;
}
