#include <stdio.h>
#include <stdlib.h>


int main(void) {
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);

	int n;
		int a[100], loc , element;
		printf("Enter number of elements: ");
		scanf("%d",&n);
		for(int i = 0 ; i < n ; i++)
		{
			scanf("%d",&a[i]);
		}



	return 0;
}
