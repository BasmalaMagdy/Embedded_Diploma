#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int a , b ;
	char c;
	printf("Enter oberator + or - or * or /: ");
	scanf("%c",&c);
	printf("Enter two operands: ");
	scanf("%d \n %d",&a , &b);



	return 0;
}
